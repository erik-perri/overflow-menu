{
  "name": "overflow-menu",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .ts src/*",
    "build-dev:types": "tsc --noEmit --watch",
    "build-dev:js": "rollup -c --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c --environment PRODUCTION",
    "build": "npm run lint && npm run build:types && npm run build:js"
  },
  "main": "dist/overflow-menu.cjs.js",
  "module": "dist/overflow-menu.esm.js",
  "browser": "dist/overflow-menu.umd.js",
  "minified": "dist/overflow-menu.umd.min.js",
  "types": "dist/types/index.d.ts",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.2",
    "rollup": "^2.10.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "typescript": "^3.9.2"
  }
}
